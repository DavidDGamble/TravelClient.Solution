@{
    ViewData["Title"] = "Destinations";
}

<h1>All Dem Places</h1>

<h3>Actions:</h3>

<p>@Html.ActionLink("Add a new destination to the site", "Create")</p>

<h3>Destinations:</h3>
<ol>
@foreach (Destination destination in Model)
{
  <li><strong>@destination.Name</strong><a href="/Destinations/Details/@(destination.DestinationId)">View Details</a></li>
}
</ol>